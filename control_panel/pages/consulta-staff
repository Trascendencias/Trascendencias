        <script>
			function activate(elem){ //ACTIVE, PARA EL MENU MOVIL
				if(elem.classList.contains('active')) elem.classList.remove('active'); 
				else elem.classList.add('active'); 
			}

        //AQUI PONES EL VALOR DE Q
        var titulo; var nuevolnk; var accionlnk;
        titulo = "Miembros"; nuevolnk = "registro-staff "; accionlnk = "consulta-staff ";



        function nuevo(){
            location.href = nuevolnk;
        }



            w3.includeHTML(); //INCLUIR DOCUMENTOS

            document.getElementById("contador").innerHTML += 456;
            document.getElementById("titulo").innerHTML += titulo;
            
            function buscar(busqueda){
                var enlistados = document.getElementsByClassName("enlistado");
                var C = 0;
                while(C < enlistados.length){
                    if(enlistados[C].innerHTML.indexOf(busqueda, 0) > 0 || enlistados[C].value.indexOf(busqueda, 0) > -1) { enlistados[C].style.display = "block"; }
                    else{ enlistados[C].style.display = "none"; }
                    C++;
                }
                return "true";
            
            }

            function masvideo(){
                var videoinput = "<input maxlength='50' name='video"+ videocount +"' class='video' required><p class='error'> *Debe llenar esta información </p>"
                document.getElementById('videos').innerHTML += videoinput;
                videocount++;
            }
            function masfoto(){
                var fotosinput = "<input type='file' width='150px' name='foto"+fotoscount+"' value='fileupload' id='image' style='display: inline;'><p class='error'> *Debe llenar esta información </p>"
                document.getElementById('fotos').innerHTML += fotosinput;
                fotoscount++;
            }
            
            function isNumberKey(evt){
                var charCode = (evt.which) ? evt.which : event.keyCode
                if (charCode > 31 && (charCode < 48 || charCode > 57)) return false;
                return true;
            }
        </script>
